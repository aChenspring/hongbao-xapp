view {
  box-sizing: border-box;
}
page {
  background-color: #fff6d0;
  font-family: "Helvetica Neue", Arial, "微软雅黑", "Hiragino Sans GB", sans-serif;
}
.loading {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.loading image {
  width: 20px;
  height: 20px;
}
.s1 {
  position: relative;
}
.s1 image {
  width: 100%;
}
.s1 view {
  position: absolute;
  width: 100%;
  line-height: 20px;
  height: 20px;
  top: 62.6%;
  text-align: center;
  color: #fff961;
}
.s2 {
  position: relative;
}
.s2 image {
  width: 100%;
}
.s2 .one {
  position: absolute;
  width: 100%;
  font-size: 16px;
  line-height: 16px;
  height: 16px;
  top: 24%;
  color: #5b3715;
  text-align: center;
  letter-spacing: 1px;
}
.s2 .two {
  position: absolute;
  width: 100%;
  font-size: 12px;
  line-height: 12px;
  height: 12px;
  top: 32%;
  color: #5b3715;
  text-align: center;
  letter-spacing: 1px;
}
.s2 .three {
  position: absolute;
  width: 100%;
  font-size: 15px;
  line-height: 30px;
  height: 30px;
  top: 56.9%;
  color: #fff961;
  text-align: center;
}
.s2 .four {
  position: absolute;
  width: 44%;
  height: 13%;
  left: 31.5%;
  top: 2.5%;
  display: flex;
  justify-content: space-between;
}
.s2 .four image {
  width: 30%;
  height: 100%;
}
.s2 .four .address {
  width: 90%;
  height: 100%;
  padding: 5px;
  font-size: 12px;
  line-height: 14px;
}
.s3 {
  margin-top: -30px;
}
.s3 image {
  width: 100%;
}
